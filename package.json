		"@testing-library/svelte": "^5.2.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
